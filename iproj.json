{
  "description": "BBS for IBM i",
  "repository": "git@github.com:bmarolleau/BBS400.git",
  "includePath": [
    "src/copybooks"
  ],
  "extensions": {
    "arcad": {
      "code": "BBS400",
      "libraryPrefix": "BBS",
      "validated": true,
      "iasp": ""
